<div class="modal fade" id="alterarSenhaModal" tabindex="-1" role="dialog" aria-labelledby="alterarSenhaModal" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="tituloModal">Alterar Senha</h5>
            </div>
            <div class="modal-body text-start">
            <form>
                <div class="form-group">
                    <label for="primeiraSenha" class="col-form-label">Digite a nova:<span class="required">*</span></label>
                    <input type="password" class="form-control" id="primeiraSenha">
                </div>
                <div class="form-group">
                    <label for="segundaSenha" class="col-form-label">Digite novamente a senha:<span class="required">*</span></label>
                    <input type="password" class="form-control" id="segundaSenha">
                </div>
            </form>
            </div>
            <div class="modal-footer">
            <button type="button" class="btn btn-secondary" id="fecharModal" data-dismiss="modal">Cancelar</button>
            <button type="button" class="btn btn-primary" id="btnAlterarSenha">Alterar</button>
            </div>
        </div>
    </div>
</div>